package principal;

import java.awt.EventQueue;

public class Principal1 {
	Ventana visualizar = new Ventana();
	
	EventQueue.invokeLater(new Runnable() {
		public void run() {
			try {
				Ventana frame = new Ventana();
				frame.setVisible(true);
			} catch (Exception e) {
				e.printStackTrace();
			}
		}
	});
}
