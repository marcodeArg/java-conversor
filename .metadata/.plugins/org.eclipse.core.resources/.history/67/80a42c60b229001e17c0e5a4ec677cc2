package principal;

import javax.swing.JOptionPane;

public class Principal {
	
	private static Object[] lista = {"algo", "algo1", "algo2", "algo3", "algo4", "algo5"}; 
	
	public static void main(String[] args) {
		String option = JOptionPane.showInputDialog(null, "Mensaje de texto", "Algo", JOptionPane.PLAIN_MESSAGE, null, lista,
				lista[0]);
		System.out.println("Seleccion " + option);
	}
}
